{
  "name": "prisma",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/service/prisma/src",
  "prefix": "lib",
  "tags": [],
  "projectType": "library",
  "targets": {
    "generate-all": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "prisma generate --schema=libs/service/prisma/src/authn/schema/schema.prisma",
          "prisma generate --schema=libs/service/prisma/src/shop/schema/schema.prisma"
        ]
      }
    },
    "generate-authn": {
      "command": "prisma generate --schema=libs/service/prisma/src/authn/schema/schema.prisma"
    },
    "generate-shop": {
      "command": "prisma generate --schema=libs/service/prisma/src/shop/schema/schema.prisma"
    },
    "data-migration": {
      "executor": "nx:run-commands",
      "configurations": {
        "add-authn-data-initial": {
          "commands": [
            "ts-node libs/service/prisma/src/authn/migrations/initial-authn-data/data-migration.ts"
          ]
        }
      }
    }
  }
}
